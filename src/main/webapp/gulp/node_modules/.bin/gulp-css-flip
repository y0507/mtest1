#!/bin/sh
basedir=`dirname "$0"`

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../gulp-css-flip/bin/gulp-css-flip" "$@"
  ret=$?
else 
  node  "$basedir/../gulp-css-flip/bin/gulp-css-flip" "$@"
  ret=$?
fi
exit $ret
